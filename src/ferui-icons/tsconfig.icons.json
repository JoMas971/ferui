{
  "compilerOptions": {
    "outDir": "../../dist/ferui-icons",
    "target": "es5",
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "removeComments": false,
    "noImplicitAny": true,
    "baseUrl": "./",
    "paths": {
      "@clr/icons": ["../../node_modules/@clr/icons"],
      "@clr/icons/shapes/*": ["../../node_modules/@clr/icons/shapes", "../../node_modules/@clr/icons/shapes/*"]
    }
  },
  "include": ["**/*.ts"]
}
