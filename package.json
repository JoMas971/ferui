    "ts-node": "~5.0.1",
    "tsickle": "^0.33.0",
    "tslib": "^1.7.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-defocus": "^2.0.5",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "engines": {
