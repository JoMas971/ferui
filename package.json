    "awesome-typescript-loader": "^5.2.1",
    "cheerio": "^1.0.0-rc.2",
    "codelyzer": "^4.5.0",
    "cpy-cli": "^1.0.1",
    "csso-cli": "^1.1.0",
    "del-cli": "^1.1.0",
    "detect-browser": "^2.4.0",
    "html-reporter": "^2.22.0",
    "husky": "^1.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-expect": "^3.8.3",
