{
  "name": "@ferui/ferui",
  "version": "0.0.2",
  "author": "iLand cloud",
  "contributors": [
    {
      "name": "Jonathan Masmejean",
      "email": "jonathan.masmejean@frigg-computing.com"
    }
  ],
  "license": "BSD-3-Clause",
  "repository": "https://github.com/ilanddev/ferui",
  "scripts": {
    "clean": "del ./dist",
    "build:icons:web": "webpack --config webpack.icons.config.js",
    "build:icons:css": "node-sass --source-map=true --precision=6 ./src/ferui-icons/ferui-icons.scss ./dist/ferui-icons/ferui-icons.css",
    "build:icons:optimize": "csso ./dist/ferui-icons/ferui-icons.css ./dist/ferui-icons/ferui-icons.min.css --map file;",
    "build:icons:scss": "cpy '**/*.scss' '../../dist/ferui-icons/' --cwd=src/ferui-icons --parents",
    "build:icons:package": "cpy ./npm/ferui-icons/package.json ./npm/ferui-icons/README.md ./dist/ferui-icons/; replace '@VERSION' $npm_package_version ./dist/ferui-icons/package.json",
    "build:icons": "npm-run-all build:icons:css build:icons:optimize build:icons:web build:icons:scss build:icons:package",
    "build:ui:css": "node-sass --include-path=node_modules --source-map=true --precision=6 --indentedSyntax=true ./src/ferui-design/scss/ferui-design.scss ./dist/ferui-design/ferui-design.css",
    "build:ui:prefix": "postcss ./dist/ferui-design/ferui-design.css -o ./dist/ferui-design/ferui-design.css",
    "build:ui:optimize": "csso ./dist/ferui-design/ferui-design.css ./dist/ferui-design/ferui-design.min.css --map file",
    "build:ui:scss": "cpy '**/*.scss' '../../dist/ferui-design/' --cwd=src/ferui-design --parents",
    "build:ui:package": "cpy ./npm/ferui-design/package.json ./npm/ferui-design/README.md ./dist/ferui-design/; replace '@VERSION' $npm_package_version ./dist/ferui-design/package.json",
    "build:ui": "npm-run-all build:ui:css build:ui:prefix build:ui:optimize build:ui:scss build:ui:package",
    "build": "npm-run-all clean build:ui build:icons",
    "build:dev": "ng build dev --prod=true --extractCss=true",
    "ng": "ng",
    "start": "ng serve dev --port 9090",
    "publish:latest": "npm publish dist/ferui-design --access public; npm publish dist/ferui-icons --access public",
    "publish:alpha": "npm publish dist/ferui-design --tag alpha --access public; npm publish dist/ferui-icons --tag alpha --access public",
    "publish:beta": "npm publish dist/ferui-design --tag beta --access public; npm publish dist/ferui-icons --tag beta --access public",
    "publish:rc": "npm publish dist/ferui-design --tag rc --access public; npm publish dist/ferui-icons --tag rc --access public"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@clr/icons": "^0.13.10",
    "@webcomponents/custom-elements": "^1.0.0",
    "core-js": "^2.5.5",
    "prismjs": "^1.6.0",
    "rxjs": "6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
