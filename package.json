    "shelljs": "^0.8.1",
    "source-map": "^0.7.2",
    "ts-api-guardian": "^0.4.1",
    "ts-node": "~5.0.1",
    "tsickle": "^0.33.0",
    "tslib": "^1.7.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-defocus": "^2.0.5",
